        "custom_liquid": "<style>\n  .lux-breadcrumb-box { padding: 1rem 0; }\n  .lux-breadcrumb-inner { display: flex; flex-direction: column; gap: 0.5rem; }\n  .lux-breadcrumb { display: flex; flex-wrap: wrap; align-items: center; font-size: 0.875rem; line-height: 1.4; }\n  .lux-breadcrumb a { text-decoration: none; }\n  .lux-breadcrumb a:hover { text-decoration: underline; }\n  .lux-breadcrumb-separator { margin: 0 0.35rem; }\n  .lux-breadcrumb .current { font-weight: 600; }\n  .lux-title { font-size: 1.8rem; line-height: 1.2; }\n\n  @media (max-width: 1024px) {\n    .lux-title { font-size: 1.5rem; }\n    .lux-breadcrumb { font-size: 0.8rem; }\n  }\n\n  @media (max-width: 640px) {\n    .lux-breadcrumb-inner { gap: 0.25rem; }\n    .lux-breadcrumb { font-size: 0.75rem; }\n    .lux-title { font-size: 1.25rem; }\n    .lux-breadcrumb .current {\n      white-space: nowrap; overflow: hidden; text-overflow: ellipsis; max-width: 100%;\n    }\n  }\n</style>\n\n{%- if collection != blank -%}\n  <div class=\"lux-breadcrumb-box page-width-content\">\n    <div class=\"lux-breadcrumb-inner\">\n      <nav class=\"lux-breadcrumb\" aria-label=\"breadcrumbs\">\n        <a href=\"{{ routes.root_url }}\">INICIO</a>\n\n        {%- comment -%} Metafields (ajusta \"custom\" si usas otro namespace) {%- endcomment -%}\n        {% assign n1      = collection.metafields.custom.n1      | default: collection.metafields.custom.N1 %}\n        {% assign n1_url  = collection.metafields.custom.n1_url  | default: collection.metafields.custom.N1_URL %}\n        {% assign n2      = collection.metafields.custom.n2      | default: collection.metafields.custom.N2 %}\n        {% assign n2_url  = collection.metafields.custom.n2_url  | default: collection.metafields.custom.N2_URL %}\n        {% assign n3      = collection.metafields.custom.n3      | default: collection.metafields.custom.N3 %}\n        {% assign n3_url  = collection.metafields.custom.n3_url  | default: collection.metafields.custom.N3_URL %}\n\n        {%- comment -%}\n          Normaliza URL a path interno:\n          - Quita protocolo\n          - Quita query/hash\n          - Si empieza con '/', se usa tal cual\n          - Si no, asume dominio/..., ignora el primer segmento y reconstruye /...\n        {%- endcomment -%}\n\n        {%- assign n1_path = '' -%}\n        {%- if n1_url != blank -%}\n          {% assign u = n1_url | strip | split: '?' | first | split: '#' | first %}\n          {% assign u_no_proto = u | remove: 'https://' | remove: 'http://' %}\n          {% assign first_char = u_no_proto | slice: 0, 1 %}\n          {% if first_char == '/' %}\n            {% assign n1_path = u_no_proto %}\n          {% else %}\n            {% assign parts = u_no_proto | split: '/' %}\n            {% assign n1_path = '' %}\n            {% for part in parts %}\n              {% unless forloop.first %}\n                {% if part != blank %}\n                  {% assign n1_path = n1_path | append: '/' | append: part %}\n                {% endif %}\n              {% endunless %}\n            {% endfor %}\n          {% endif %}\n        {%- endif -%}\n\n        {%- assign n2_path = '' -%}\n        {%- if n2_url != blank -%}\n          {% assign u = n2_url | strip | split: '?' | first | split: '#' | first %}\n          {% assign u_no_proto = u | remove: 'https://' | remove: 'http://' %}\n          {% assign first_char = u_no_proto | slice: 0, 1 %}\n          {% if first_char == '/' %}\n            {% assign n2_path = u_no_proto %}\n          {% else %}\n            {% assign parts = u_no_proto | split: '/' %}\n            {% assign n2_path = '' %}\n            {% for part in parts %}\n              {% unless forloop.first %}\n                {% if part != blank %}\n                  {% assign n2_path = n2_path | append: '/' | append: part %}\n                {% endif %}\n              {% endunless %}\n            {% endfor %}\n          {% endif %}\n        {%- endif -%}\n\n        {%- assign n3_path = '' -%}\n        {%- if n3_url != blank -%}\n          {% assign u = n3_url | strip | split: '?' | first | split: '#' | first %}\n          {% assign u_no_proto = u | remove: 'https://' | remove: 'http://' %}\n          {% assign first_char = u_no_proto | slice: 0, 1 %}\n          {% if first_char == '/' %}\n            {% assign n3_path = u_no_proto %}\n          {% else %}\n            {% assign parts = u_no_proto | split: '/' %}\n            {% assign n3_path = '' %}\n            {% for part in parts %}\n              {% unless forloop.first %}\n                {% if part != blank %}\n                  {% assign n3_path = n3_path | append: '/' | append: part %}\n                {% endif %}\n              {% endunless %}\n            {% endfor %}\n          {% endif %}\n        {%- endif -%}\n\n        {%- comment -%} N1 {%- endcomment -%}\n        {% if n1 != blank %}\n          <span class=\"lux-breadcrumb-separator\">/</span>\n          {% if n1_path != blank %}\n            <a href=\"{{ n1_path }}\">{{ n1 }}</a>\n          {% else %}\n            <span>{{ n1 }}</span>\n          {% endif %}\n        {% endif %}\n\n        {%- comment -%} N2 {%- endcomment -%}\n        {% if n2 != blank %}\n          <span class=\"lux-breadcrumb-separator\">/</span>\n          {% if n2_path != blank %}\n            <a href=\"{{ n2_path }}\">{{ n2 }}</a>\n          {% else %}\n            <span>{{ n2 }}</span>\n          {% endif %}\n        {% endif %}\n\n        {%- comment -%} N3 {%- endcomment -%}\n        {% if n3 != blank %}\n          <span class=\"lux-breadcrumb-separator\">/</span>\n          {% if n3_path != blank %}\n            <a href=\"{{ n3_path }}\">{{ n3 }}</a>\n          {% else %}\n            <span>{{ n3 }}</span>\n          {% endif %}\n        {% endif %}\n\n        <span class=\"lux-breadcrumb-separator\">/</span>\n        <span class=\"current\">{{ collection.title }}</span>\n      </nav>\n\n      <h1 class=\"lux-title\">{{ collection.title }}</h1>\n    </div>\n  </div>\n{%- endif -%}",

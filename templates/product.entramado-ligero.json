                "custom_liquid_wYfRkd": {
                  "type": "custom-liquid",
                  "name": "t:names.custom_liquid",
                  "settings": {
                    "custom_liquid": "<style>\n/*====================\n  PRECIOS (PRICING BOXES)\n======================*/\n.pricing-section {\n  display: flex;\n  justify-content: center;\n  margin: 0 auto .5rem;\n}\n\n.pricing-boxes {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: center;\n  gap: 1.5rem;\n  max-width: 960px;\n}\n\n.pricing-box {\n  background-color: #d9d9d9;\n  padding: .5rem;\n  text-align: center;\n  min-width: 200px;\n  border-radius: 8px;\n  box-shadow: 0 4px 6px rgba(0,0,0,0.1);\n  flex: 1 1 200px;\n}\n\n.pricing-basic{\nbackground: #F0EAE0;\n}\n.pricing-standard{\nbackground: #DDD2C0;\n}\n.pricing-passive{\nbackground: #C4B49A;\n}\n\n.pricing-box h4 {\n  margin: 0;\n  margin-bottom: 0.5rem;\n  text-transform: uppercase;\n}\n\n.pricing-box p {\n  margin: 0;\n}\n</style>\n\n<!-- PRECIOS -->\n<div class=\"pricing-section\">\n  <div class=\"pricing-boxes\">\n    <div class=\"pricing-box pricing-basic\">\n      <h4>BASIC</h4>\n      <p class=\"pricing-price\">{{ product.metafields.custom.price_basic.value | money }}</p>\n    </div>\n    <div class=\"pricing-box pricing-standard\">\n      <h4>STANDARD</h4>\n      <p class=\"pricing-price\">{{ product.metafields.custom.price_standard.value | money }}</p>\n    </div>\n    <div class=\"pricing-box pricing-passive\">\n      <h4>PASSIVE</h4>\n      <p class=\"pricing-price\">{{ product.metafields.custom.price_passive.value | money }}</p>\n    </div>\n  </div>\n</div>"

                "custom_liquid_wYfRkd": {
                  "type": "custom-liquid",
                  "name": "t:names.custom_liquid",
                  "settings": {
                    "custom_liquid": "<style>\n/* ============================\n   PRICING (minimal / clean)\n   - Responsive grid\n   - Accessible + SEO-friendly\n   - Scalable (3–16+ cards)\n   ============================ */\n\n.pricing{ margin: 0 auto 1rem; padding: .25rem 0; }\n.pricing__container{ max-width: 1080px; margin: 0 auto; padding: 0 .75rem; }\n\n.pricing__header{ margin: .25rem 0 .75rem; }\n.pricing__title{\n  margin: 0;\n  font-size: clamp(1.15rem, 1.6vw, 1.45rem);\n  letter-spacing: .01em;\n}\n.pricing__subtitle{\n  margin: .25rem 0 0;\n  font-size: .95rem;\n  opacity: .85;\n  line-height: 1.35;\n  max-width: 70ch;\n}\n\n.pricing__grid{\n  display: grid;\n  grid-template-columns: repeat(3, minmax(0, 1fr));\n  gap: 1rem;\n}\n@media (max-width: 980px){\n  .pricing__grid{ grid-template-columns: repeat(2, minmax(0, 1fr)); }\n}\n@media (max-width: 640px){\n  .pricing__grid{ grid-template-columns: 1fr; }\n}\n\n.pricing-card{\n  border-radius: 14px;\n  padding: .95rem;\n  border: 1px solid rgba(0,0,0,.08);\n  box-shadow: 0 8px 18px rgba(0,0,0,.08);\n  background: #F3EEE4;\n  position: relative;\n  overflow: hidden;\n}\n\n.pricing-card[data-tone=\"sand\"]{ background: #F0EAE0; }\n.pricing-card[data-tone=\"taupe\"]{ background: #DDD2C0; }\n.pricing-card[data-tone=\"olive\"]{ background: #C4B49A; }\n.pricing-card[data-tone=\"stone\"]{ background: #E9E6DF; }\n.pricing-card[data-tone=\"smoke\"]{ background: #E3E0D9; }\n\n.pricing-card__badge{\n  position: absolute;\n  top: .75rem;\n  right: .75rem;\n  background: rgba(17,17,17,.92);\n  color: #fff;\n  padding: .25rem .5rem;\n  border-radius: 999px;\n  font-size: .78rem;\n  font-weight: 650;\n  letter-spacing: .01em;\n}\n\n.pricing-card__title{\n  margin: 0 0 .3rem;\n  font-size: 1.02rem;\n  letter-spacing: .02em;\n  text-transform: uppercase;\n}\n\n.pricing-card__label{\n  margin: 0;\n  font-size: .88rem;\n  opacity: .85;\n  line-height: 1.3;\n}\n\n.pricing-card__price{\n  margin: .35rem 0 .25rem;\n  line-height: 1.05;\n}\n.pricing-card__amount{\n  font-weight: 800;\n  font-size: clamp(1.25rem, 2.1vw, 1.65rem);\n}\n.pricing-card__unit{\n  margin-left: .35rem;\n  font-weight: 650;\n  font-size: .95rem;\n  opacity: .9;\n  white-space: nowrap;\n}\n\n.pricing-card__desc{\n  margin: .35rem 0 0;\n  font-size: .95rem;\n  line-height: 1.35;\n}\n\n.pricing__notes{\n  margin-top: .75rem;\n  padding: .75rem .85rem;\n  border-radius: 12px;\n  border: 1px dashed rgba(0,0,0,.18);\n  background: rgba(255,255,255,.35);\ntext-align:justify;\n}\n.pricing__note{\n  margin: .2rem 0;\n  font-size: .88rem;\n  line-height: 1.4;\n  opacity: .95;\n}\n</style>\n\n{%- assign pricing_cards = product.metafields.custom.pricing_cards.value | default: blank -%}\n\n{%- comment -%} Legacy fallback (3 campos antiguos) {%- endcomment -%}\n{%- assign p_core = product.metafields.custom.price_basic.value | default: blank -%}\n{%- assign p_clean = product.metafields.custom.price_standard.value | default: blank -%}\n{%- assign p_select = product.metafields.custom.price_passive.value | default: blank -%}\n{%- assign d_core = product.metafields.custom.descripcion_basic | default: blank -%}\n{%- assign d_clean = product.metafields.custom.descripcion_standard | default: blank -%}\n{%- assign d_select = product.metafields.custom.descripcion_passive | default: blank -%}\n\n{%- assign has_any = false -%}\n{%- if pricing_cards != blank and pricing_cards.size > 0 -%}\n  {%- assign has_any = true -%}\n{%- elsif p_core != blank or p_clean != blank or p_select != blank -%}\n  {%- assign has_any = true -%}\n{%- endif -%}\n\n{%- if has_any -%}\n<section class=\"pricing\" id=\"precios\" aria-labelledby=\"pricing-title\">\n  <div class=\"pricing__container\">\n\n    <header class=\"pricing__header\">\n      <h2 class=\"pricing__title\" id=\"pricing-title\">\n        Precios orientativos de {{ product.title | escape }} por m²\n      </h2>\n      <p class=\"pricing__subtitle\">\n        Comparativa rápida por niveles de suministro. <strong>IVA NO INCLUIDO</strong>.\n      </p>\n    </header>\n\n    <div class=\"pricing__grid\" role=\"list\" aria-label=\"Niveles de precios\">\n      {%- assign low_price = blank -%}\n      {%- assign high_price = blank -%}\n      {%- assign offer_count = 0 -%}\n\n      {%- if pricing_cards != blank and pricing_cards.size > 0 -%}\n\n        {%- for card in pricing_cards -%}\n          {%- assign title = card.title | default: card.name | default: \"Tarima\" -%}\n          {%- assign subtitle = card.subtitle | default: \"Precio por m² · tarima\" -%}\n          {%- assign price_raw = card.price | default: blank -%}\n          {%- assign desc = card.description | default: blank -%}\n          {%- assign badge = card.badge | default: blank -%}\n          {%- assign tone = card.tone | default: \"sand\" -%}\n\n          {%- comment -%}\n            Normalización de precio:\n            - Si viene en céntimos (>= 1000) => divide/100\n            - Si viene en euros (ej: 79.00) => se queda\n          {%- endcomment -%}\n          {%- assign price_num = price_raw | plus: 0 -%}\n          {%- assign price_eur = price_num -%}\n          {%- if price_raw != blank and price_num >= 1000 -%}\n            {%- assign price_eur = price_num | divided_by: 100.0 -%}\n          {%- endif -%}\n\n          {%- if price_raw != blank -%}\n            {%- assign offer_count = offer_count | plus: 1 -%}\n            {%- if low_price == blank or price_eur < low_price -%}{%- assign low_price = price_eur -%}{%- endif -%}\n            {%- if high_price == blank or price_eur > high_price -%}{%- assign high_price = price_eur -%}{%- endif -%}\n          {%- endif -%}\n\n          {%- assign price_cents_display = price_eur | times: 100 | round -%}\n\n          <article class=\"pricing-card\" role=\"listitem\" data-tone=\"{{ tone | escape }}\">\n            {%- if badge != blank -%}\n              <p class=\"pricing-card__badge\">{{ badge | escape }}</p>\n            {%- endif -%}\n\n            <h3 class=\"pricing-card__title\">{{ title | escape }}</h3>\n            <p class=\"pricing-card__label\">{{ subtitle | escape }}</p>\n\n            {%- if price_raw != blank -%}\n              <p class=\"pricing-card__price\" aria-label=\"Precio {{ title | escape }}\">\n                <span class=\"pricing-card__amount\">{{ price_cents_display | money_without_currency }}</span>\n                <span class=\"pricing-card__unit\">€ / m²</span>\n              </p>\n            {%- endif -%}\n\n            {%- if desc != blank -%}\n              <p class=\"pricing-card__desc\">{{ desc }}</p>\n            {%- endif -%}\n          </article>\n        {%- endfor -%}\n\n      {%- else -%}\n\n        {%- comment -%} Legacy fallback: Core / Clean / Select {%- endcomment -%}\n\n        {%- if p_core != blank -%}\n          {%- assign n = p_core | plus: 0 -%}\n          {%- assign eur = n -%}\n          {%- if n >= 1000 -%}{%- assign eur = n | divided_by: 100.0 -%}{%- endif -%}\n          {%- assign cents = eur | times: 100 | round -%}\n          {%- assign offer_count = offer_count | plus: 1 -%}\n          {%- if low_price == blank or eur < low_price -%}{%- assign low_price = eur -%}{%- endif -%}\n          {%- if high_price == blank or eur > high_price -%}{%- assign high_price = eur -%}{%- endif -%}\n\n          <article class=\"pricing-card\" role=\"listitem\" data-tone=\"sand\">\n            <h3 class=\"pricing-card__title\">Outdoor Core</h3>\n            <p class=\"pricing-card__label\">Precio por m² · tarima</p>\n            <p class=\"pricing-card__price\" aria-label=\"Precio Outdoor Core\">\n              <span class=\"pricing-card__amount\">{{ cents | money_without_currency }}</span>\n              <span class=\"pricing-card__unit\">€ / m²</span>\n            </p>\n            {%- if d_core != blank -%}<p class=\"pricing-card__desc\">{{ d_core }}</p>{%- endif -%}\n          </article>\n        {%- endif -%}\n\n        {%- if p_clean != blank -%}\n          {%- assign n = p_clean | plus: 0 -%}\n          {%- assign eur = n -%}\n          {%- if n >= 1000 -%}{%- assign eur = n | divided_by: 100.0 -%}{%- endif -%}\n          {%- assign cents = eur | times: 100 | round -%}\n          {%- assign offer_count = offer_count | plus: 1 -%}\n          {%- if low_price == blank or eur < low_price -%}{%- assign low_price = eur -%}{%- endif -%}\n          {%- if high_price == blank or eur > high_price -%}{%- assign high_price = eur -%}{%- endif -%}\n\n          <article class=\"pricing-card\" role=\"listitem\" data-tone=\"taupe\">\n            <h3 class=\"pricing-card__title\">Outdoor Clean</h3>\n            <p class=\"pricing-card__label\">Precio por m² · tarima</p>\n            <p class=\"pricing-card__price\" aria-label=\"Precio Outdoor Clean\">\n              <span class=\"pricing-card__amount\">{{ cents | money_without_currency }}</span>\n              <span class=\"pricing-card__unit\">€ / m²</span>\n            </p>\n            {%- if d_clean != blank -%}<p class=\"pricing-card__desc\">{{ d_clean }}</p>{%- endif -%}\n          </article>\n        {%- endif -%}\n\n        {%- if p_select != blank -%}\n          {%- assign n = p_select | plus: 0 -%}\n          {%- assign eur = n -%}\n          {%- if n >= 1000 -%}{%- assign eur = n | divided_by: 100.0 -%}{%- endif -%}\n          {%- assign cents = eur | times: 100 | round -%}\n          {%- assign offer_count = offer_count | plus: 1 -%}\n          {%- if low_price == blank or eur < low_price -%}{%- assign low_price = eur -%}{%- endif -%}\n          {%- if high_price == blank or eur > high_price -%}{%- assign high_price = eur -%}{%- endif -%}\n\n          <article class=\"pricing-card\" role=\"listitem\" data-tone=\"olive\">\n            <h3 class=\"pricing-card__title\">Outdoor Select</h3>\n            <p class=\"pricing-card__label\">Precio por m² · tarima</p>\n            <p class=\"pricing-card__price\" aria-label=\"Precio Outdoor Select\">\n              <span class=\"pricing-card__amount\">{{ cents | money_without_currency }}</span>\n              <span class=\"pricing-card__unit\">€ / m²</span>\n            </p>\n            {%- if d_select != blank -%}<p class=\"pricing-card__desc\">{{ d_select }}</p>{%- endif -%}\n          </article>\n        {%- endif -%}\n\n      {%- endif -%}\n    </div>\n\n    <div class=\"pricing__notes\" aria-label=\"Notas sobre precios\">\n      <p class=\"pricing__note\">\n        <strong>IVA NO INCLUIDO.</strong> Los precios pueden variar según grado, largos, mecanizado, certificaciones, stock y acabado.\n        Si quieres una propuesta ajustada a tu obra, consúltanos y te informamos sin compromiso.\n      </p>\n      <p class=\"pricing__note\">\n        Precio orientativo de la lama (sin subestructura, herrajes, transporte ni instalación).\n        La elección de rastrel, tornillería inox y ventilación del soporte condiciona el resultado final.\n      </p>\n    </div>\n\n    {%- if offer_count > 0 and low_price != blank and high_price != blank -%}\n      {%- assign low_json = low_price | round: 2 | replace: \",\", \".\" -%}\n      {%- assign high_json = high_price | round: 2 | replace: \",\", \".\" -%}\n      <script type=\"application/ld+json\">\n      {\n        \"@context\": \"https://schema.org\",\n        \"@type\": \"Product\",\n        \"name\": \"{{ product.title | escape }}\",\n        \"url\": \"{{ shop.url }}{{ product.url }}\",\n        \"offers\": {\n          \"@type\": \"AggregateOffer\",\n          \"priceCurrency\": \"EUR\",\n          \"lowPrice\": {{ low_json }},\n          \"highPrice\": {{ high_json }},\n          \"offerCount\": \"{{ offer_count }}\"\n        }\n      }\n      </script>\n    {%- endif -%}\n\n  </div>\n</section>\n{%- endif -%}"

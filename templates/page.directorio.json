      "settings": {
        "custom_liquid": "<style>\n  .link-grid {\n    display: grid;\n    grid-template-columns: repeat(auto-fit, minmax(clamp(140px, 22vw, 1fr), 1fr));\n    max-width: 100%;\n    gap: clamp(0.75rem, 2vw, 1.5rem);\n    list-style: none;\n    padding: 0;\n    margin: 2rem 0;\n  }\n\n  .link-grid li {\n    margin-bottom: 0.5rem;\n  }\n\n  /* Máximo de 4 columnas */\n  @media screen and (min-width: 1200px) {\n    .link-grid {\n      grid-template-columns: repeat(3, 1fr);\n    }\n  }\n\n  /* Tablets horizontales y pantallas medianas: 3 columnas */\n  @media screen and (min-width: 750px) and (max-width: 1199px) {\n    .link-grid {\n      grid-template-columns: repeat(3, 1fr);\n    }\n  }\n\n  /* Tablets verticales: 2 columnas */\n  @media screen and (max-width: 749px) {\n    .link-grid {\n      grid-template-columns: repeat(2, 1fr);\n    }\n  }\n\n  /* Móviles muy pequeños: 1 columna */\n  @media screen and (max-width: 450px) {\n    .link-grid {\n      grid-template-columns: 1fr;\n    }\n  }\n</style>\n<div class=\"page-width-content\">\n<!-- PÁGINAS -->\n<h3>PÁGINAS</h3>\n<ul class=\"link-grid\">\n  {% assign sorted_pages = pages | sort: \"metafields.custom.N2\" %}\n  {% for page in sorted_pages %}\n{% assign n2      = page.metafields.custom.n2 | default: page.metafields.custom.N2 %}\n    <li><a href=\"{{ page.url }}\">{% if n2 != blank %}\n          {{ n2 }} - \n      {% endif %}{{ page.title }}</a></li>\n  {% endfor %}\n</ul>\n\n<!-- PRODUCTOS -->\n<h3>PRODUCTOS</h3>\n<ul class=\"link-grid\">\n  {% assign sorted_products = collections.all.products | sort: \"title\" %}\n  {% for product in sorted_products %}\n    <li><a href=\"{{ product.url }}\">{{ product.title | upcase }}</a></li>\n  {% endfor %}\n</ul>\n\n<!-- POLÍTICAS -->\n<h3>POLÍTICAS</h3>\n<ul class=\"link-grid\">\n  {% if shop.privacy_policy %}\n    <li><a href=\"{{ shop.privacy_policy.url }}\">POLÍTICA DE PRIVACIDAD</a></li>\n  {% endif %}\n  {% if shop.refund_policy %}\n    <li><a href=\"{{ shop.refund_policy.url }}\">POLÍTICA DE REEMBOLSO</a></li>\n  {% endif %}\n  {% if shop.shipping_policy %}\n    <li><a href=\"{{ shop.shipping_policy.url }}\">POLÍTICA DE ENVÍO</a></li>\n  {% endif %}\n  {% if shop.terms_of_service %}\n    <li><a href=\"{{ shop.terms_of_service.url }}\">TÉRMINOS DEL SERVICIO</a></li>\n  {% endif %}\n  {% if shop.contact_policy %}\n    <li><a href=\"{{ shop.contact_policy.url }}\">POLÍTICA DE CONTACTO</a></li>\n  {% endif %}\n</ul>\n</div>",
